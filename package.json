{
  "name": "water-electron",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir --icon=water.ico",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/Nicole-tu/water-electron.git",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.2.0",
    "electron-builder": "^22.8.0",
    "electron-notifications": "^1.0.0",
    "node-notifier": "^8.0.0"
  },
  "build": {
    "appId": "com.nicole-tu.water-reminder",
    "productName": "Water Reminder",
    "directories": {
      "output": "build"
    },
    "mac": {
      "target": "default",
      "icon": "water.ico"
    },
    "win": {
      "target": "nsis",
      "icon": "water.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "water.ico"
    }
  }
}
